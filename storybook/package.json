{
  "name": "@kami-ui/storybook",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && storybook build",
    "build:story": "storybook build",
    "dev": "pnpm dev:storybook",
    "dev:react": "vite",
    "dev:storybook": "storybook dev -p 3000",
    "lint": "prettier \"{src,public,.storybook}/**/*.{ts,js,tsx,jsx,html,css,mdx,svg}\" --write && eslint \"{src,public,.storybook}/**/*.{ts,js,tsx,jsx,html,css,mdx,svg}\"",
    "lint:fix": "prettier \"{src,public,.storybook}/**/*.{ts,js,tsx,jsx,html,css,mdx,svg}\" --write && eslint \"{src,public,.storybook}/**/*.{ts,js,tsx,jsx,html,css,mdx,svg}\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@kami-ui/react-components": "workspace:*",
    "@kami-ui/react-theme": "workspace:*",
    "@kami-ui/theme-shop": "workspace:*",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4",
    "@kami-ui/eslint-config": "workspace:*",
    "@kami-ui/tsconfigs": "workspace:*",
    "@storybook/addon-a11y": "^9.0.8",
    "@storybook/addon-docs": "^9.0.8",
    "@storybook/addon-vitest": "^9.0.8",
    "@storybook/react-vite": "^9.0.8",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.27.0",
    "storybook": "^9.0.8",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
